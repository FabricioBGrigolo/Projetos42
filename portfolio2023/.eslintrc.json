{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-typescript",
        "next/core-web-vitals",
        "plugin:react/recommended",
        "plugin:import/typescript",
        "plugin:prettier/recommended"
    ],
    "overrides": [
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "ecmaFeatures": {
          "jsx": true
        },
        "project": "./tsconfig.json"
    },
    "plugins": [
        "react", "@typescript-eslint", "prettier"
    ],
    "rules": {
    }
}
